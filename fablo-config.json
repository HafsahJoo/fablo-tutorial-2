{
  "$schema": "https://github.com/hyperledger-labs/fablo/releases/download/2.1.0/schema.json",
  "global": {
    "fabricVersion": "2.5.9",
    "tls": true,
    "engine": "docker",
    "peerDevMode": false
  },
  "orgs": [
    {
      "organization": {
        "name": "Orderer",
        "domain": "orderer.example.com"
      },
      "orderers": [
        {
          "groupName": "group1",
          "type": "raft",
          "instances": 3
        }
      ],
      "tools": {}
    },
    {
      "organization": {
        "name": "Citizens",
        "domain": "citizens.example.com"
      },
      "peer": {
        "instances": 2,
        "db": "CouchDb"
      },
      "tools": {}
    },
    {
      "organization": {
        "name": "Health",
        "domain": "health.example.com"
      },
      "peer": {
        "instances": 1,
        "db": "CouchDb"
      },
      "tools": {}
    },
    {
      "organization": {
        "name": "Police",
        "domain": "police.example.com"
      },
      "peer": {
        "instances": 1,
        "db": "CouchDb"
      },
      "tools": {}
    },
    {
      "organization": {
        "name": "Electoral",
        "domain": "electoral.example.com"
      },
      "peer": {
        "instances": 1,
        "db": "CouchDb"
      },
      "tools": {}
    },
    {
      "organization": {
        "name": "Civil",
        "domain": "civil.example.com"
      },
      "peer": {
        "instances": 1,
        "db": "CouchDb"
      },
      "tools": {}
    }
  ],
  "channels": [
    {
      "name": "main-channel",
      "orgs": [
        {
          "name": "Citizens",
          "peers": [
            "peer0",
            "peer1"
          ]
        },
        {
          "name": "Health",
          "peers": [
            "peer0"
          ]
        },
        {
          "name": "Police",
          "peers": [
            "peer0"
          ]
        },
        {
          "name": "Electoral",
          "peers": [
            "peer0"
          ]
        },
        {
          "name": "Civil",
          "peers": [
            "peer0"
          ]
        }
      ]
    },
    {
      "name": "citizens-health",
      "orgs": [
        {
          "name": "Citizens",
          "peers": [
            "peer0"
          ]
        },
        {
          "name": "Health",
          "peers": [
            "peer0"
          ]
        }
      ]
    },
    {
      "name": "citizens-civil",
      "orgs": [
        {
          "name": "Citizens",
          "peers": [
            "peer0"
          ]
        },
        {
          "name": "Civil",
          "peers": [
            "peer0"
          ]
        }
      ]
    },
    {
      "name": "citizens-police",
      "orgs": [
        {
          "name": "Citizens",
          "peers": [
            "peer0"
          ]
        },
        {
          "name": "Police",
          "peers": [
            "peer0"
          ]
        }
      ]
    },
    {
      "name": "citizens-electoral",
      "orgs": [
        {
          "name": "Citizens",
          "peers": [
            "peer0"
          ]
        },
        {
          "name": "Electoral",
          "peers": [
            "peer0"
          ]
        }
      ]
    }
  ],
  "chaincodes": [
    {
      "name": "identity-chaincode",
      "version": "0.0.1",
      "lang": "node",
      "channel": "main-channel",
      "directory": "./chaincodes/identity-chaincode",
      "endorsement": ""
    },
    {
      "name": "health-chaincode",
      "version": "0.0.1",
      "lang": "node",
      "channel": "citizens-health",
      "directory": "./chaincodes/health-chaincode",
      "endorsement": ""
    },
    {
      "name": "civil-chaincode",
      "version": "0.0.1",
      "lang": "node",
      "channel": "citizens-civil",
      "directory": "./chaincodes/civil-chaincode",
      "endorsement": ""
    },
    {
      "name": "police-chaincode",
      "version": "0.0.1",
      "lang": "node",
      "channel": "citizens-police",
      "directory": "./chaincodes/police-chaincode",
      "endorsement": ""
    },
    {
      "name": "electoral-chaincode",
      "version": "0.0.1",
      "lang": "node",
      "channel": "citizens-electoral",
      "directory": "./chaincodes/electoral-chaincode",
      "endorsement": ""
    }
  ],
  "hooks": {
    "postGenerate": "echo 'Network configuration generated!'",
    "postStart": "echo 'Network started!'",
    "preDown": "echo 'Stopping network...'",
    "postDown": "echo 'Network stopped!'"
  }
}